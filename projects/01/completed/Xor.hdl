/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=b, out=Nandab);
    Nand(a=a, b=Nandab, out=NandaNandab);
    Nand(a=Nandab, b=b, out=NandbNandab);
    Nand(a=NandaNandab, b=NandbNandab, out=out);
}